package day0220;

public class ForTest_10 {

	public static void main(String[] args) {
		//1. 1~10까지 가로로 출력
		int i;
		for(i=1;i<=10;i++)
			System.out.print(i+"  ");
		//2. 10~1까지 숫자 가로로 출력
		System.out.println();
		for(i=10;i>=1;i--)
			System.out.print(i+"  ");
		System.out.println();
		//Hello1 Hello2 Hello3 출력
		for(i=1;i<=3;i++) {
			System.out.print("Hello"+i+"  ");
		}
		System.out.println();
		//1,4,7......50까지 3의배수
		for(i=1;i<=50;i+=3)
		{
			System.out.print(i+"  ");
		}
		System.out.println();

		//1 2 3 4 6 7 8 9 11...20까지 중 5의 배수 없이 출력
		for(i=1;i<=20;i++)
		{
			if(i%5==0)
				continue;  //i++로 이동
			System.out.print(i+" ");
		}
		System.out.println();
		
		
	}

}
