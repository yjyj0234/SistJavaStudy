package day0310;

import java.awt.Color;
import java.awt.Container;

import javax.swing.ImageIcon;
import javax.swing.JFrame;

public class SwingIcon5 extends JFrame{

	Container cp;
	//각각의 이미지아이콘 (.GIF까지 써줘야함)
	ImageIcon icon1=new ImageIcon("C:\\sist0217\\image\\swingimage\\LEFT.GIF");
	ImageIcon icon2=new ImageIcon("C:\\sist0217\\image\\swingimage\\leftDown.GIF");
	ImageIcon icon3=new ImageIcon("C:\\sist0217\\image\\swingimage\\leftRoller.GIF");
	ImageIcon icon4=new ImageIcon("C:\\sist0217\\image\\swingimage\\45-pandadog.GIF");
	public SwingIcon5(String title) {
		super(title);  //JFrame 부모생성자를 통해서 창제목을 통해서 제목을 볼수 있게한다
		
	
		this.setBounds(0, 10, 900, 500);
		
	
		cp=this.getContentPane();
		cp.setBackground(new Color(0,0,0));
		
		//창을 보여주게 하겠다
		this.setVisible(true);

		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		new SwingIcon5("스윙 아이콘 #5");
		
	}
}

