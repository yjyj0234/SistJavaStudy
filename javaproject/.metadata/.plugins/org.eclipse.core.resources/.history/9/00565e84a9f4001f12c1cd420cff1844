package day0227;

//MyInfo에 내 정보 넣기
class MyInfo{
	private String name;                   
	private int age;
	
	//setter 메서드
	public void setName(String name)
	{
		this.name=name;
	}
	public void setAge(int age) 
	{
		this.age=age;
	}
	//getter 메서드..조회할 목적이라 return 필요
	public getName(String name)
	{
		
	}
	
}
/////////////////////////////////////////////
public class Ex3WorkerMethod {

	public static void main(String[] args) {

		MyInfo my1=new MyInfo();
		my1.setName("이수연");
		my1.setAge(22);
		
		
	}

}
