package day0224;

import java.util.Random;

public class RandonTest_02 {

	public static void main(String[] args) {
		// 자바에서 난수를 구하는 방법
		//Math.random()
		//Random 이라는 클래스를 생성해서 구하는 방법
		
		Random r=new Random();
		
		System.out.println("0에서 9사이의 난수 5개 발생");
		for(int i=1;i<=5;i++)
		{
			double n=r.nextInt();
			System.out.print(n+"\t");
			
		}
		System.out.println("========================");
		

	}

}
