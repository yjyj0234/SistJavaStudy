package day0227;

class Happy{
	
	public Happy() {
		System.out.println("생성자1번");
	}
	public Happy(String msg) {
		System.out.println("문자열"+msg+"를 받는 생성자");
	}
	public Happy(int age) {
		System.out.println("숫자"+age+"를 받는 생성자");
	}
}

