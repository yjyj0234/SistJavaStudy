package day0228;

class Student
{
	private String stuName;
	private String hp;
	private int score;
	
	//명시적생성자3개인자
	public Student(String name,String hp,int score)
	{
		this.stuName=name;
		this.hp=hp;
		this.score=score;
		
	}
}
