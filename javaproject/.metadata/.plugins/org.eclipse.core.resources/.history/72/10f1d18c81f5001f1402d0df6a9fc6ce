package day0228;

class MyBag{
	String bagName;
	int bagPrice;
	String bagColor;
}
////////////////////////////////////////////
public class Ex6CallByRef {
	
	public static void bagInfoIn(MyBag bag) 
	{
		/*
		 * MyBag 멤버 초기화
		 */
		bag.bagName="백팩";
		bag.bagPrice=65000;
		bag.bagColor="블랙";
		
	}
	//출력
	public static void bagInfoOut(MyBag bag) 
	{
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
