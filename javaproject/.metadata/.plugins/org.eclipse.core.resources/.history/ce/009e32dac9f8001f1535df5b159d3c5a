package day0304;

//interface ==>interface : extends
//class ==>interface:implements
//class ==> class:extends
interface InterA{
	
	public void play();
	public void write();
	
}

interface InterB{
	
	public void draw();
}

//2개의 인터페이스를 구현하는 클래스. 인터페이스는 다중구현이 가능
class InterImpl implements InterA,InterB{

	

	@Override
	public void play() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void write() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void draw() {
		// TODO Auto-generated method stub
		
	}
	
}
public class Ex12InterfaceMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
