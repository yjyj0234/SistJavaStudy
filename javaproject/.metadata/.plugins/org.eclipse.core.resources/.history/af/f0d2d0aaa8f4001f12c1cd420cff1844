package day0227;

//MyInfo에 내 정보 넣기
class MyInfo{
	String name;                   
	private int age;
}
/////////////////////////////////////////////
public class Ex3WorkerMethod {

	public static void main(String[] args) {

		MyInfo my1=new MyInfo();
		my1.name="이수연";
		System.out.println(my1.name);
		
	}

}
