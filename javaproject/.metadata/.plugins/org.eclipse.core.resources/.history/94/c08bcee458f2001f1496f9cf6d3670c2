package day0224;

public class ArrayTest_04 {

	public static void main(String[] args) {
		// 배열선언..기본형 배열은 객체로 인식
		
		int [] arr,arr2;
		arr=new int[3];  //3개의 정수에 대한 메모리 할당
		arr2=new int[5];  //할당만 해도 값은 0으로 초기화
		
		//배열에 값 넣기...초기화
		arr[0]=100;                   //arr  100|50|200      arr2 0|0|0|0|0
		arr[1]=50;
		arr[2]=200;             
		
	}

}
