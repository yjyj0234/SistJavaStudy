package day0221;

public class GuguBreak_06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i=2;i<=9;i++) {
			{
				for(int j=1;j<=9;j++)
				{
					if(j==5)
						break;
					System.out.println(i+"X"+j+"="+i*j);
				}
				System.out.println();
			}
		}
	}

}
