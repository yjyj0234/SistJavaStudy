package day0305;

public class Ex17StringToken {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String msg="red|pink|yellow|black|white";
		//String msg="red,pink,yellow,black,white";
		
		System.out.println(msg);
		
		// |로 분리해서 출력
		String [] arr=msg.split("|");
	}

}
