package day0227;

class Jungbo
{
	private String name;
	private int age;
	private String job;
	private String hobby;
	
	public String getName() 
	{
		return name;
	}
	public void setName(String name) 
	{
		this.name = name;
	}
	public int getAge() 
	{
		return age;
	}
	public void setAge(int age) 
	{
		this.age = age;
	}
	public String getJob() 
	{
		return job;
	}
	public void setJob(String job) 
	{
		this.job = job;
	}
	public String getHobby() 
	{
		return hobby;
	}
	public void setHobby(String hobby) 
	{
		this.hobby = hobby;
	}
	
	//출력 메서드 
	public void name() 
	{
		
	}
}
