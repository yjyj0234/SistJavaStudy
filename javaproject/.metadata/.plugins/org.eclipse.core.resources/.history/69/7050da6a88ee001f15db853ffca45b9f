package day0219;

import java.util.Scanner;

public class IfTest_09 {

	public static void main(String[] args) {
		// 두수를 입력하여 더 큰값 구하기
		Scanner sc=new Scanner(System.in);
		int x,y;
		
		System.out.println("두수를 입력하세요");
		x=sc.nextInt();
		y=sc.nextInt();
		
		System.out.println("입력한 두수: "+x+", "+y);
		
		
		
	}

}
