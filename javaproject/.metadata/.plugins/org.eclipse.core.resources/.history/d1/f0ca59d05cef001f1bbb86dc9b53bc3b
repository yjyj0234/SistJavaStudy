package day0220;

public class ForWhileReview_14 {

	public static void sumTest1() {
		
		//for문으로 1~5까지 더하기
		int sum=0;
		for(int i=1;i<=5;i++)
		{
			sum+=i; //sum=sum+i
		}
		System.out.println("총합계는 "+sum+"입니다");
	}
	
	
	public static void main(String[] args) {
		// 
		//sumTest1();    //썸 메서드를 메인메서드에 호출
	}

}
