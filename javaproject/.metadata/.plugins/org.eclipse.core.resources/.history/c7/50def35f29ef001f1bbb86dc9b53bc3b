package day0220;

public class SwitchTest_02 {

	public static void main(String[] args) {
		// switch 이용예제
		
		int n=1;
		
		//break가 없을 경우 끝까지 실행
		
		switch(n)
		{
		case 1:
			System.out.println("n is 1");
			break;
		case 2:
			System.out.println("n is 2");
			break;
		case 3:
			System.out.println("n is 3");
			break;
		default:
			System.out.println("n 은 3보다 크다");
			break;
		}
		

	}

}
