package day0225;

public class RndSortLotto {

	public static void main(String[] args) {
		
		int [] lotto=new int[6];
		
		//임의의 수를 로또변수에 발생 시켜서 넣어준다
		for(int i=0;i<lotto.length;i++)
		{
			lotto[i]=(int)(Math.random()*45)+1;  //1~45중의 랜덤수 발생
			System.out.println("뽑은 값: "+lotto[i]);
		}
		
		

	}

}
