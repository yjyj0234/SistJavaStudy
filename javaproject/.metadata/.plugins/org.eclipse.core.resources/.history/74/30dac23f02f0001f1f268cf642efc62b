package day0221;

import java.util.Scanner;

public class ForSumScan_08 {

	public static void main(String[] args) {
		// 점수를 3번반복해서 입력 후 합계를 구하시오(1~100까지 입력할 것!!!)
		// 1번째 점수를 입력하세요(1~100)
		// 해당범위를 벗어나면 잘못입력해습니다. 반복문 진행하되 한번 뺀다
		Scanner sc=new Scanner(System.in);
		
		int num;
		int sum=0,cnt=0;
		
		for(cnt=1;cnt<=3;cnt++)
		{
			System.out.println(cnt+"번째 점수를 입력: ");
		}
		
		
		
		

	}

}
