package day0220;

public class ForWhileReview_14 {

	public static void sumTest1() {
		
		//for문으로 1~5까지 더하기
		int sum=0;
		for(int i=1;i<=5;i++)
		{
			sum+=i; //sum=sum+i
		}
		System.out.println("총합계는 "+sum+"입니다");
	}
	public static void sumTest2() {
		
		//1~10까지 중 홀수의 합 1 3 5 7 9 
		int sum=0;
		for(int i=1;i<=10;i++)
		{
			if(i%2==0)
				continue;
			
		}
	}
	
	public static void main(String[] args) {
		// 
		//sumTest1();    //썸 메서드를 메인메서드에 호출
		
	}

}
