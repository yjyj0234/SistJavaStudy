package day0226;

public class ArrayDacha_04 {

	public static void main(String[] args) {
		// 다차원 배열 선언과 동시에 초기화
		
		int [][] arr= {
						{22,33},
						{44,55,66},
						{77,88,99,100}
					};
		//출력
		System.out.println("총행갯수: "+ arr.length);
		System.out.println("0번행 열갯수: "+arr[0].length);
		System.out.println("1번행 열갯수: "+arr[1].length);
		System.out.println("2번행 열갯수: "+arr[2].length);
	}

}
