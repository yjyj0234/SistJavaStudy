package day0219;

public class IfTest_07 {

	public static void main(String[] args) {
		
		int n=3;
		
		if(n<5) 
			System.out.println("n은 5보다 큽니다");
		
		System.out.println("프로그램 종료");
		
		///////////////////////////////////
		
		int a=9;
		
		if(a%2==1)
			System.out.println("a는 홀수입니다");
		else
			System.out.println("a는 짝수입니다");
		
	}

}