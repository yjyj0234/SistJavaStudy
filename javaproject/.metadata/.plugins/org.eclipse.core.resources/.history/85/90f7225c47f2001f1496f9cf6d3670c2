package day0224;

public class ForReviewStar_01 {

	public static void main(String[] args) {
		
		System.out.println("**다중 For문으로 star(*) 출력하기**");
		
		//5행 5열로 star 출력
		for(int i=1;i<=5;i++)    //**행 갯수
		{
			for(int j=1;j<=5;j++)  //열 갯수
			{
				System.out.print("*");
				
			}
			System.out.println();
		}
		//5행 5열star(1행:1열 2행:2열까지...)
	}

}
