package day0227;

public class Ex11MemberTest {

	public static void main(String[] args) {
		// 멤버 2명 객체 생성 후 출력
		Ex11Member mem1=new Ex11Member();
		Ex11Member mem2=new Ex11Member();
		
		Ex11Member.ban="쌍용고등학교";
		mem1.setMemberData("손흥민", "남", "서울");
		mem1.getMemberData();
		mem2.setMemberData("김연아", "여", "서울");
		mem2.getMemberData();
	}

}
