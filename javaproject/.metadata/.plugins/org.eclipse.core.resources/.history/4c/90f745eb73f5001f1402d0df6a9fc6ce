package day0228;

class MyShop{
	
	private String sangName;
	private int su;
	private int dan;
	
	//생성자 인자3개
	public MyShop(String sangName, int su, int dan)
	{
		this.sangName=sangName;
		this.su=su;
		this.dan=dan;
	}
	//출력
	public void writeSangpum() 
	{
		System.out.println();
	}
}
