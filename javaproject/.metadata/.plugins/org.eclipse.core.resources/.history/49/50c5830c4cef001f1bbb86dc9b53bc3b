package day0220;

public class ForTest_10 {

	public static void main(String[] args) {
		//1. 1~10까지 가로로 출력
		int i;
		for(i=1;i<=10;i++)
			System.out.print(i+"  ");
		//2. 10~1까지 숫자 가로로 출력
		System.out.println();
		for(i=10;i>=1;i--)
			System.out.print(i+"  ");
		System.out.println();
		//Hello1 Hello2 Hello3 출력
		for(i=1;i<=3;i++) {
			System.out.print("Hello"+i+"  ");
		}
	}

}
