package day0227;

class Const{
	
	int num;     //0
	
	//디폴트 생성자는 보이지 않지만 이미 존재한다
	
	//인자있는 생성자가 필요하다면 반드시 생성자를 만든다
	
	public Const(int num) {
		this.num=num;
		System.out.println("인자있는 생성자 호출");
		
	}
}

////////////////////////////////////////////
public class Ex2ConstTest {

	public static void main(String[] args) {
		// 
		Const con1=new Const(300);
		
		
	}

}
