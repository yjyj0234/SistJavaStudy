package day0227;

class Orange{
	public Orange() {
		//만들지 않아도 자동존재
		System.out.println("디폴트 생성자 호출");
	}
	
}
///////////////////////////////
class Apple{
	
}
///////////////////////////////
class Banana{

}
public class Ex4ConstTest {

	public static void main(String[] args) {
		// 생성자는 객체생성시 자동호출되는 메서드
		//
		
		Orange or=new Orange();
		System.out.println(Orange);
	}

}
